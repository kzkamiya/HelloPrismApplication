<?xml version="1.0" encoding="UTF-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             xmlns:behaviors="clr-namespace:Prism.Behaviors;assembly=Prism.Forms" 
             xmlns:i18n="clr-namespace:HelloPrismApplication.i18n;assembly=HelloPrismApplication" 
             xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core" 
             xmlns:android="clr-namespace:Xamarin.Forms.PlatformConfiguration.AndroidSpecific;assembly=Xamarin.Forms.Core" 
             xmlns:windows="clr-namespace:Xamarin.Forms.PlatformConfiguration.WindowsSpecific;assembly=Xamarin.Forms.Core" 
             Title="{Binding Title}" 
             x:Class="HelloPrismApplication.Views.ViewA">
    <StackLayout>
        <Label Text="ViewA!!" VerticalOptions="Center" HorizontalOptions="Center"/>
        <Button Text="Navigate" Command="{Binding NextPage}"/>
        <Label Text="{Binding TodoItem.Name}" VerticalOptions="Center" HorizontalOptions="Center"/>
        <Label Text="{Binding Name}" VerticalOptions="Center" HorizontalOptions="Center"/>
        <Label Text="TEST!!" VerticalOptions="Center" HorizontalOptions="Center"/>
    </StackLayout>
</ContentPage>